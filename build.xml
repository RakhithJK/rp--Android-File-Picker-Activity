<project name="android-filepicker" default="jar" basedir=".">
  <description>A file picker for android</description>
  <!-- set global properties for this build -->
  <property name="src" location="src" />
  <property name="bin" location="bin" />

  <target name="jar">
	  <jar destfile="android-filepicker.jar" basedir="bin/" includes="com/**" excludes="**/R*.class **/MainActivity.class" />
  </target>
</project>
